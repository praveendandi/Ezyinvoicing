 "html": "\n<!DOCTYPE html>\n<html lang=\"en\">\n\n<head>\n  <style type=\"text/css\">\n    table {\n      page-break-inside: initial;\n    }\n\n    tr {\n      page-break-inside: initial;\n      page-break-after: auto;\n    }\n\n    thead {\n      display: table-header-group;\n\n    }\n\n    tfoot {\n      display: table-footer-group;\n    }\n  </style>\n\n  <style>\n    .print-format {\n      padding: 10px;\n\n    }\n    .auth-sign{\n      text-align: center;\n      vertical-align: middle;\n    }\n    body {\n      font-size: 10px;\n      background: #fff;\n      width: 100%;\n      box-sizing: content-box;\n    }\n\n    @media print {\n      @page {\n        margin: 0;\n        size: landscape !important;\n        /* DIN A4 standard, Europe */\n      }\n    }\n\n    .text-center {\n      text-align: center;\n    }\n\n    .text-ryt {\n      text-align: right;\n    }\n\n    .p-0 {\n      padding: 0px !important;\n    }\n\n    .table-bordered td,\n    .table-bordered th {\n      padding: 0;\n      border: 0 !important;\n    }\n\n    .table td,\n    .table th {\n      border-top: 0;\n      padding: 0px;\n      border-collapse: collapse;\n    }\n\n    .mt-10 {\n      margin-top: 10px;\n    }\n\n    .ml-20 {\n      margin-left: 20px;\n      font-size: 10px;\n    }\n\n    .ml-10 {\n      margin-left: 10px;\n      font-size: 13px;\n    }\n\n    * {\n      padding: 0;\n      margin: 0;\n      font-family: sans-serif;\n      border-spacing: 0;\n    }\n\n    table {\n      width: 100%;\n      text-align: left;\n      box-sizing: 0;\n      line-height: 10px;\n    }\n\n    .goods table {\n      border: 1px solid #00000085;\n      padding: 3px 3px !important;\n      font-size: 10px;\n      text-transform: capitalize;\n      vertical-align: top;\n    }\n\n    .goods td,\n    .goods th {\n      border: 1px solid #00000085;\n      padding: 2px 2px !important;\n      font-size: 10px;\n      text-transform: none;\n      vertical-align: top;\n\n    }\n\n    .goods3 table {\n      border: 1px solid #00000085;\n      padding: 3px 3px !important;\n      font-size: 10px;\n      text-transform: none;\n      vertical-align: top;\n    }\n\n    .goods3 td,\n    .goods3 th {\n      border: 1px solid #00000085;\n      padding: 2px 2px !important;\n      font-size: 10px;\n      text-transform: none;\n      vertical-align: top;\n    }\n\n    .goods3 th {\n      background-color: #b3b3b396 !important;\n      -webkit-print-color-adjust: exact;\n    }\n\n    .goods h4 {\n      line-height: 13px;\n    }\n\n    .goods h3 {\n      font-size: 16px;\n      margin-top: 5px;\n    }\n\n    .goods div {\n      line-height: 13px;\n    }\n\n    .border {\n      border: 1px solid #000000a3;\n    }\n\n    .goods2 {\n      padding: 0 !important;\n      border: none !important;\n    }\n\n    .goods2 table {\n      border: none;\n    }\n\n    .goods2 td,\n    .goods2 th {\n      border-right: none;\n      padding: 1px 1px !important;\n      font-size: 10px;\n      text-transform: none;\n      vertical-align: top;\n    }\n\n    .goods2 td {\n      border-left: none !important;\n      border-bottom: none;\n      border-top: none !important;\n    }\n    \n    .goods3 table{\n      border-top: 1px solid #00000085;\n    }\n\n    .guest-sign {\n      vertical-align: bottom !important;\n      text-align: center;\n    }\n\n    .m-10 {\n      margin: 10px;\n    }\n\n    .mt-30 {\n\n      margin-left: 25px;\n      font-size: 10px;\n    }\n\n    .w-50 {\n      width: 50%;\n    }\n\n    .w-25 {\n      width: 25%;\n    }\n\n    .mt-20 {\n      margin-top: 20px;\n      font-size: 10px;\n    }\n\n    h4 {\n      margin: 0px !important;\n      font-size: 12px;\n      text-decoration: underline;\n    }\n\n\n    .ml-20 {\n      margin-left: 20px;\n    }\n\n    .bor-l {\n      border-left: none !important;\n    }\n\n    .bor-r {\n      border-right: none !important;\n    }\n\n    .bor-b {\n      border-bottom: none !important;\n    }\n\n    .bor-t {\n      border-top: none !important;\n    }\n\n    .bor-n {\n      border: none !important;\n    }\n\n    .p-0 {\n      padding: 0 !important;\n    }\n    .ml-4{\n      margin-left: 4px;\n    }\n    .chalethotel{\n      display:flex; justify-content: space-between;\n    }\n    .tax-inv{\n        margin-top:5px;\n        margin-bottom:5px;\n    }\n    \n  </style>\n\n</head>\n\n<body>\n  <div class=\"print-format\">\n    <div class=\"table-bor\">\n      <table class=\"goods table\">\n        <tbody>\n           <tr>\n               <td colspan=\"4\" class=\"text-center bor-b\" >\n                   {% if ( invoice_category=='Tax Invoice') %}\n                   <h3 class=\"tax-inv\">\n                       Tax Invoice\n                   </h3>\n                  {% endif %} \n               </td>\n           </tr>\n          <tr>\n            <td class=\"bor-r bor-t auth-sign\">\n              <img class=\"img-fluid \" src=\"{{company_logo_base}}\" alt=\"\" style='width: 200px'> \n            </td>\n            <td class=\"bor-l bor-r bor-t\">\n              <table class=\"goods2 table mt-0\">\n                <tbody>\n                  <tr>\n                    <td colspan=\"2\">\n                        {% set company_details=frappe.get_doc('company', company) %}\n                        \n                        <p>fnkmdsnfkmdsnfsakm</p>\n                        \n                      <div>\n                        <p><strong> {{company_details.legal_name}}  </strong><br/>\n                        {{company_details.address_1}},<br> {{company_details.address_2}}, <br>{{company_details.location}},<br> {{company_details.pincode}}</p>\n                        \n                      </div>\n                    </td>  \n                  </tr>\n                  <tr>\n                    <td>\n                      <strong>GSTIN No. :</strong>\n                    </td>\n                    <td>{{company_details.gst_number}}</td>\n                  </tr>\n                  <tr>\n                    <td>\n                      <strong>Tel :</strong>\n                    </td>\n                    <td>{{company_details.phone_number}}</td>\n                  </tr>\n                  <tr>\n                    <td>\n                      <strong>E-Mail :</strong>\n                    </td>\n                    <td>{{company_details.email}}</td>\n                  </tr>\n                  <tr>\n                    <td>\n                      <strong>PAN No. :</strong>\n                    </td>\n                    <td>{{company_details.gst_number[2:12]}}</td>\n                    <td>\n                  </tr>\n                 \n                  <tr>\n                      {% if ( invoice_category=='Tax Invoice' or invoice_category=='Debit Invoice' ) %}\n                        <td>\n                            <strong>IRN Date :</strong>\n                        </td>\n                        <td>{{ack_date}}</td>\n                        {% elif ( invoice_category=='Credit Invoice' ) %}\n                        <td>\n                            <strong>IRN Date :</strong>\n                        </td>\n                        <td>{{credit_ack_date}}</td>\n                      {% endif %}\n                  </tr>\n                </tbody>\n              </table>\n              <div style=\"margin-left:5px\">\n                  {% if ( invoice_category=='Tax Invoice' or invoice_category=='Debit Invoice' ) %}\n                    <span>\n                        <strong>IRN :</strong>\n                    </span>\n                    <span>{{irn_number}}</span>\n                {% elif ( invoice_category=='Credit Invoice' ) %}   \n                    <span>\n                        <strong>IRN :</strong>\n                    </span>\n                    <span>{{credit_irn_number}}</span>\n                {% endif %}    \n              </div>\n            </td>\n            <td class=\"bor-l bor-r bor-t\">\n              <table class=\"goods2 table my-0\">\n                <t