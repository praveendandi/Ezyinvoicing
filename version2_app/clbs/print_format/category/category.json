 "html": "<!DOCTYPE html>\n<html lang=\"en\">\n<head>\n    <meta charset=\"UTF-8\">\n    <meta http-equiv=\"X-UA-Compatible\" content=\"IE=edge\">\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n <title style=\"text-align:center\">Food IRD</title>\n    \n    <style>\n         body{\n            font-size: 12px !important;\n            font-family: Arial, Helvetica, sans-serif; !important;\n        }\n        .text-right{\n            text-align:right;\n        }\n        .text-center{\n            text-align:center;\n        }\n        .row{\n            display : flex;\n        }\n      \n      \n       .print-format{\n            padding: 0px;\n            margin: 0;\n        }\n        \n        .print-table{\n            border: 0;\n        }\n        \n       .print-table table{\n           width: 100%;\n       }\n       \n       .print-table th, .print-table td{\n            border: none;\n        }\n        \n        .printable2 table {\n            width: 100%;\n        }\n        \n        .printable2 th, .printable2 td{\n            border: none;\n        }\n        \n        .printable3 table{\n             width:100% !important;\n            /*min-width : 80%;*/\n            /*max-width : 95%;*/\n        }\n        .printable3 table, .printable3 th, .printable3 td{\n            border: solid 1px black !important;\n            border-collapse: collapse;\n            \n        }\n        .printable3 table thead tr th{\n            padding: 2px 4px !important;\n        }\n        .printable3 table tr td{\n            padding: 2px 4px !important;\n        }\n        \n        .print-friendly{\n            margin-top: 10px;\n        }\n        table.print-friendly tr td, table.print-friendly tr th {\n        page-break-inside: avoid;\n        }\n        \n        }\n        .totalamount{\n          border : solid 1px black;\n        }\n         @page{\n            size:portrait; \n            margin: 1cm; \n         }\n        #footer-html{\n            position:fixed;\n            bottom:-30px;\n        }\n        \n        .right-div{\n            width: 30%; \n            float: right; \n        }\n        \n        .signature {\n            float: right;\n            position: relative;\n            top: 80px;\n            background-color: #f2f2f2;\n            padding: 5px;\n        }\n        \n        .signature img{\n            max-width: 250px;\n        }\n        \n        .clearfix {\n            clear: both\n        }\n    </style>\n</head>\n<body>\n    <div class=\"print-format\">\n        <!--{% set image='0.0.0.0:8000/private/files/372855b2cqr1a3f7f.png'%}-->\n    \n        {% set doc_data =  frappe.db.sql(\"\"\"select DATE_FORMAT(`tabSummaries`.from_date,'%d-%m-%Y') as from_date,`tabSummaries`.po_number,`tabSummaries`.terms_and_conditions,`tabSummaries`.footer,`tabTaxPayerDetail`.legal_name,`tabTaxPayerDetail`.company,`tabTaxPayerDetail`.gst_number,`tabTaxpayer Locations`.location,`tabTaxpayer Locations`.address,`tabTaxpayer Locations`.pin_code,`tabTaxPayerDetail`.state_code,`tabTaxpayer Locations`.city from `tabSummaries` inner join `tabTaxPayerDetail` on `tabTaxPayerDetail`.gst_number=`tabSummaries`.tax_payer_details inner join `tabTaxpayer Locations` on `tabTaxpayer Locations`.name=`tabSummaries`.location where `tabSummaries`.tax_payer_details='{}' and `tabSummaries`.name='{}'\"\"\".format(tax_payer_details,name),as_dict=1)%}\n        {% set doc_data = doc_data|first %}\n        {% set company_details=frappe.db.get_value('company',{'name':doc_data.company},['summary_header','summary_terms_and_conditions','company_name','summary_footer','gst_number','state_code'],as_dict=1)%}\n        <div class='print-table'>\n            <table class='table'>\n                <tr>\n                    <td style=\" text-align: left;\" ><img src={{base}} style=\"width: 150px; \"/></td>\n                    <td style=\"text-align: left; vertical-align: middle;\">\n                        <h2 style='margin-b'>{{company_details.company_name}}</h2>\n                    </td>\n                </tr>\n            </table>\n        </div>\n        <div class='printable2'>\n            <table class='table'>\n                <tr>\n                    <td colspan='2' style='text-align: center'>\n                        <h3 style=\"text-align: center\">{{category}}</h3>\n                    </td>\n                </tr>\n                <tr>\n                    <td>\n                        <p class=\"mb-0\">Date :- {{ frappe.format_value(frappe.utils.now(), {'fieldtype': 'Date'}) }}</p>\n                    </td>\n                    <td style=\" text-align: right\">\n                        <p class='mb-0'>PO Number: {{doc_data.po_number if doc_data.po_number else \"\"}}</p>\n                    </td>\n                </tr>\n                <tr>\n                    <td colspan='2'>\n                         <p class=\"m-0\">To,</p>\n                        <p>\n                            <b class=\"fw-bold\">{{doc_data.legal_name}}</b><br>\n                           {{doc_data.legal_name}} <br>\n                            {%set items=[doc_data.address,doc_data.location,doc_data.city,doc_data.state,doc_data.pin_code]%}\n                            {{ items|reject('equalto', None)|join(',') }}\n                        </p>\n                    </td>\n                </tr>\n                <tr>\n                    <td colspan='2'>\n                    <p>\n                        GST Number : {{doc_data.gst_number}}<br>\n                        State Code : {{doc_data.state_code}}\n                    </p>\n            \n                    <p style=\" margin-top:15px\">\n                        Dear Sir/ Madam, <br>\n                        Sub: Please find enclosed the following bills for settlement\n                    </p>\n                    </td>\n                </tr>\n            </table>\n        </div>\n\n        <div class=\"\" >\n            {% set doc_data = frappe.db.sql(\"\"\"select DATE_FORMAT(`tabSummaries`.from_date,'%d-%m-%Y') as from_date,`tabSummaries`.terms_and_conditions,`tabSummaries`.footer,`tabTaxPayerDetail`.legal_name,`tabTaxPayerDetail`.company,`tabTaxPayerDetail`.gst_number,`tabTaxpayer Locations`.location,`tabTaxpayer Locations`.address,`tabTaxpayer Locations`.pin_code,`tabTaxPayerDetail`.state_code,`tabTaxpayer Locations`.city from `tabSummaries` inner join `tabTaxPayerDetail` on `tabTaxPayerDetail`.gst_number=`tabSummaries`.tax_payer_details inner join `tabTaxpayer Locations` on `tabTaxpayer Locations`.name=`tabSummaries`.location where `tabSummaries`.tax_payer_details='{}' and `tabSummaries`.name='{}'\"\"\".format(tax_payer_details,name),as_dict=1)%}\n            <!--{% set doc_data=doc_data[0]%}-->\n            <!--frappe.db.get_all(doctype=\"Summaries\",fields=[`Summaries`.from_date,`TaxPayerDetail`.legal_name,`tabTaxPayerDetail`.gst_number\",\"`tabTaxPayerDetail`.address_1\",\"`tabTaxPayerDetail`.address_2\",\"`tabTaxPayerDetail`.pincode\",\"`tabTaxPayerDetail`.state_code\",\"`TaxPayerDetail`.location\"])%}-->\n            <!--<p class=\"fw-bold\">Date :- {{ frappe.format_value(frappe.utils.now(), {'fieldtype': 'Date'}) }}</p>-->\n            {% set company_details=frappe.db.get_value('company',{'name':doc_data.company},['summary_header','summary_terms_and_conditions','summary_footer'],as_dict=1)%}\n            <!--<p class=\"fw-bold\">{{doc_data.guest_name}}</p>-->\n        </div>\n        <div class=\"\" >\n            <div style=\"width:100%\">\n       \n\n        {% set total_cgst = 0 %}\n        {% set total_sgst = 0 %}\n\n        {% set base_total = 0 %}\n        {% set total_amount = 0%}\n\n        <div class='printable3'>\n            <table class=\"table print-friendly\">\n              {% set tempData = frappe.db.get_all(doctype=\"Summary Breakups\",filters={\"category\":category,\"summaries\":name},fields=[\"`tabSummary Breakup Details`.date as date\",\"`tabSummary Breakup Details`.bill_no\",\"`tabSummary Breakup Details`.invoice_no\",\"`tabSummary Breakup Details`.particulars\",\"`tabSummary Breakup Details`.sac_code\",\"`tabSummary Breakup Details`.base_amount\",\"`tabSummary Breakup Details`.tax\",\"`tabSummary Breakup Details`.cgst\",\"`tabSummary Breakup Details`.sgst\",\"`tabSummary Breakup Details`.igst\",\"`tabSummary Breakup Details`.amount\"]) %}\n              <!--{% set tempData = tempData %}-->\n              {% set tempData2 = tempData|first %}\n            \n                <thead>\n                    \n                     <tr>\n              {% for k,x in tempData2.items()%}\n              \n            <th>{{ k }}</th>\n            {% endfor %}\n         \n            </tr>\n                    \n            \n                </thead>\n                <tbody>\n                    \n                 \n                    \n                        {% for v in tempData%}\n                        <tr>\n                             {% for k,x in v.items()%}\n    \n                            {% if k=='amount' %}\n                              <td style=\"text-align:right; max-width: 100px\">{{x|int}}</td>\n                               {% elif 'base_amount' in k %}\n                              <td style=\"text-align:right; max-width: 100px\">{{x|int}}</td>\n                               {% elif 'cgst' in k %}\n                              <td style=\"text-align:right; max-width: 100px\">{{x|int}}</td>\n                               {% elif 'sgst' in k %}\n                              <td style=\"text-align:right; max-width: 100px\">{{x|int}}</td>\n                               {% elif 'igst' in k %}\n                              <td style=\"text-align:right; max-width: 100px\">{{x|int}}</td>\n                               {% elif 'date' in k %}\n                              <td style=\"text-align:left; max-width: 100px\">{{x.strftime('%d-%m-%Y')}}</td>\n                              {% elif 'tax' in k %}\n                              <td style='max-width: 100px'>{{(x|str)+'%'}}</td>\n                             {% else%}\n                             <td style='max-width: 200px'>{{x}}</td>\n                             {% endif %}\n                            {% endfor %}\n                        </tr>\n                         \n                        {% endfor %}\n                        <tr>\n                           \n                            <td colspan='5'  style=\"text-align: right;\"><strong>Total Amount</strong></td>\n                            \n                            <td  style=\"text-align: right;\"><b>{{(tempData|sum(attribute='base_amount'))|int}}</b></td>\n                            <td></td>\n                            <td  style=\"text-align: right;\"><b>{{(tempData|sum(attribute='cgst'))|int}}</b></td>\n                            <td  style=\"text-align: right;\"><b>{{(tempData|sum(attribute='sgst'))|int}}</b></td>\n                            <td  style=\"text-align: right;\"><b>{{(tempData|sum(attribute='igst'))|int}}</b></td>\n                            <td  style=\"text-align: right;\"><b>{{(tempData|sum(attribute='amount'))|int}}</b></td>\n                            \n                         \n                        </tr>\n                        <!-- group items by item_code //-->\n                  \n                </tbody>\n            </table>\n        </div>\n        <!--<strong>Total:</strong>{{ total }}-->\n        </div>\n        </div>\n       \n        <div class=\"\" style=\"display:flex;margin-top:25px;\">\n    <div class=\"\">\n        <p>\n            {{ doc_data.terms_and_conditions if doc_data.terms_and_conditions else company_details.summary_terms_and_conditions }}\n        </p>\n    </div>\n</div>\n        \n        <div class=\"\" style=\" margin-top:15px\">\n            <!--<div class=\"\" style=\"width:50%;  margin-top:45px\">-->\n            <!--    <p>-->\n                \n            <!--      