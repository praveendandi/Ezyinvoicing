                            <td>IFSC CODE</td>\n                             <td>{{item.ifsc_code}}</td>\n                            </tr>\n                             <tr>\n                                 <td>MICR CODE</td>\n                                 <td>{{item.micr_code}}</td>\n                             </tr>\n                        \n                        {% endfor %}\n                        <!--<tr>-->\n                        <!--     <td>BANK NAME</td>-->\n                        <!--     <td>{{bank_data[0].bank_name}}</td>-->\n                        <!--</tr>-->\n                        <!--<tr>-->\n                        <!-- <td>BRANCH ADDRESS</td>-->\n                        <!-- <td>RAJBHAVAN ROAD,SOMAJIGUDA,HYD-500082</td>-->\n                        <!--</tr>-->\n                        <!--<tr>-->\n                             <!--<td>BRANCH CODE</td>-->\n                        <!--     <td>446</td>-->\n                        <!--</tr>-->\n                        <!--<tr>-->\n                             <!--<td>BANK A/C NAME</td>-->\n                        <!--     <td>CYBERABAD CONVENTION CENTER PVT.LTD</td>-->\n                        <!--</tr>-->\n                         <!--<tr>-->\n                             <!--<td>BANK A/C NO.</td>-->\n                         <!--    <td>5435456456456</td>-->\n                         <!--</tr>-->\n                        <!-- <tr>-->\n                        <!--     <td>IFSC CODE</td>-->\n                        <!--     <td>SCBL8498359</td>-->\n                        <!--</tr>-->\n                        <!-- <tr>-->\n                        <!--     <td>MICR CODE</td>-->\n                        <!--     <td>3435437543</td>-->\n                        <!-- </tr>-->\n                    </tbody>\n                 </table>\n            </div>\n        </div>\n       \n       <div class=\"\" style=\"display:flex;margin-top:25px\">\n    <div class=\"\">\n        <p>\n              <b>\n                    {% set company_details=frappe.db.get_value('company',{'name':doc_data.company},['summary_header','summary_terms_and_conditions','company_name','summary_footer','gst_number','state_code'],as_dict=1)%}\n                     (GST No : {{company_details.gst_number}}, PAN No:{{company_details.gst_number[2:12]}}, State Code : {{company_details.state_code}})\n              </b>\n        </p>\n    </div>\n</div>\n       \n    </div>\n    \n    <footer id=\"footer-html\">\n       <p style=\"font-size:11px;font-weight:bold;text-align:center\">This is a computer generated summary no signature required.</p>\n   </footer>\n</body>\n</html>",
