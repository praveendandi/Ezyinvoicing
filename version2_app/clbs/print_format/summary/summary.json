 "html": "<!DOCTYPE html>\n<html lang=\"en\">\n<head>\n    <meta charset=\"UTF-8\">\n    <meta http-equiv=\"X-UA-Compatible\" content=\"IE=edge\">\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n    <title>summery</title>\n    \n    <style>\n        body{\n            font-size: 13px !important;\n        }\n        table,th,td{\n            border: solid 1px black;\n            border-collapse: collapse;\n            padding-top: 0px !important;\n            padding-bottom: 0px !important;\n        }\n        table.print-friendly tr td, table.print-friendly tr th {\n        page-break-inside: avoid;\n    }\n    </style>\n</head>\n<body>\n    <div class=\"\">\n        <h6 class=\"text-center\">Summary</h6>\n        <div class=\"d-flex justify-content-between\">\n            {% set doc_data = frappe.db.sql(\"\"\"select `tabSummaries`.from_date,`tabTaxPayerDetail`.legal_name,`tabTaxPayerDetail`.gst_number,`tabTaxPayerDetail`.address_1,`tabTaxPayerDetail`.address_2,`tabTaxPayerDetail`.pincode,`tabTaxPayerDetail`.state_code,`tabTaxPayerDetail`.location from `tabSummaries` inner join `tabTaxPayerDetail` on `tabTaxPayerDetail`.gst_number=`tabSummaries`.tax_payer_details\"\"\",as_dict=1)%}\n            <!--{% set doc_data=doc_data[0]%}-->\n            <!--frappe.db.get_all(doctype=\"Summaries\",fields=[`Summaries`.from_date,`TaxPayerDetail`.legal_name,`tabTaxPayerDetail`.gst_number\",\"`tabTaxPayerDetail`.address_1\",\"`tabTaxPayerDetail`.address_2\",\"`tabTaxPayerDetail`.pincode\",\"`tabTaxPayerDetail`.state_code\",\"`TaxPayerDetail`.location\"])%}-->\n            <p class=\"fw-bold\">Date :- {{doc_data.from_date}}</p>\n            <!--<p class=\"fw-bold\">{{doc_data.guest_name}}</p>-->\n        </div>\n        <p class=\"m-0\">To,</p>\n        <p>\n            <b class=\"fw-bold\">{{doc_data.legal_name}}</b><br>\n           {{doc_data.legal_name}} <br>\n            {{doc_data.address_1}}, {{doc_data.address_2}}, {{doc_data.location}}, {{doc_data.pincode}},\n        </p>\n\n        <p>\n           {{doc_data.gst_number}}<br>\n            State Code : {{doc_data.state_code}}\n        </p>\n            \n\n        <p>\n            Dear Sir/ Madam, <br>\n            Sub: Please find enclosed the following bills for settlement\n        </p>\n\n        <table class=\"table print-friendly\">\n          {% set tempData = frappe.db.get_all(doctype=\"Summary Breakups\",fields=[\"date\",\"category\",\"invoice_type\",\"amount\"])\n          %}\n          \n          {% set tempData2 = tempData|first %}\n        \n            <thead>\n                <!--<tr>-->\n                    \n                <!--    <th>Sr .No</th>-->\n                <!--    <th>Date</th>-->\n                <!--    <th></th>-->\n                <!--    <th>Service Type</th>-->\n                <!--    <th>InvoiceType</th>-->\n                <!--    <th>Amount</th>-->\n                <!--</tr>-->\n                \n                 <tr>\n          {% for k,x in tempData2.items()%}\n          \n       \n        <th>{{ k }}</th>\n    \n                {% endfor %}\n            \n     \n        </tr>\n                \n        \n            </thead>\n            <tbody>\n                \n             \n                \n                    {% for v in tempData%}\n                    \n                  \n                    <tr>\n                         {% for k,x in v.items()%}\n                         <td>{{x}}</td>\n {% endfor %}\n                       \n                    </tr>\n                     \n                    {% endfor %}\n              \n                \n\n              \n            </tbody>\n        </table>\n\n        \n        <div class=\"row\">\n            <div class=\"col-6\">\n                <p>\n                    Please release the payment immediately after receipt of the bill and \n                    Payment should be made in favour of \u201cCyberabad Convention Centre Pvt. Ltd.\u201d<br>\n                    Invoice queries or disputes must be addressed to the Hotel in writing within two(2) working days of receipt of invoice.<br>\n                    Issue the TDS certificate in favour of \"Cyberabad Convention Centre Private Limited.''<br>\n                    Interest @ 18% per annum will be charged on overdue amount.<br>\n                    Thank you for your patronage <br>\n                    Yours faithfully <br>\n                    For Cyberabad Convention Centre Pvt. Ltd <br>\n                    Sundara Varma <br>\n                    Credit Manager <br>\n                    Email Id: h6182-gl2@accor.com <br>\n                    Cel No :- 9642326201 <br>\n\n                    (GST No : 36AACCC2725K1ZQ, PAN No:AACCC2725K,'CIN U55109TG2003PTC041544,Telangana State Code : 36)\n\n                </p> \n            </div>\n            <div class=\"col-6\">\n                <table class=\"table\">\n                    {% set bank_data = frappe.db.get_all(doctype=\"Bank Details\",fields=[\"*\"])\n                    %}\n                    <thead>\n                        <tr>\n                            <th colspan=\"2\" class=\"text-center\">Bank Details</th>\n                        </tr>\n                    </thead>\n                    <tbody>  \n                        {% for item in bank_data %}\n                        <TR>\n                        <tr>\n                           <TD class=\"BANK NAME\">BANK NAME</TD>\n                           <TD class=\"BANK NAME\">{{item.bank_name}}</TD>\n                           </tr>\n                           <tr>\n                               <td class=\"BRANCH ADDRESS\">BRANCH ADDRESS</TD>\n                               <TD class=\"BRANCH ADDRESS\">{{item.address}}</TD>\n                           </tr>\n                           <tr>\n                               <td>BRANCH CODE</td>\n                                <TD class=\"BRANCH CODE\">{{item.branch_code}}</TD>\n                           </tr>\n                           <tr>\n                               <td>BANK A/C NAME</td>\n                               <TD class=\"BANK A/C NAME\"><SPAN>{{item.bank_ac_name}}</SPAN></TD>\n                           </tr>\n                           <tr>\n                               <td>BANK A/C NO.</td>\n                               <TD class=\"BANK A/C NO.\"><SPAN>{{item.bank_ac_number}}</SPAN></TD>\n                           </tr>\n                            <tr>\n                             <td>IFSC CODE</td>\n                             <td>{{item.ifsc_code}}</td>\n                            </tr>\n                             <tr>\n                                 <td>MICR CODE</td>\n                                 <td>{{item.micr_code}}</td>\n                             </tr>\n                        </TR>\n                        {% endfor %}\n                        <!--<tr>-->\n                        <!--     <td>BANK NAME</td>-->\n                        <!--     <td>{{bank_data[0].bank_name}}</td>-->\n                        <!--</tr>-->\n                        <!--<tr>-->\n                        <!-- <td>BRANCH ADDRESS</td>-->\n                        <!-- <td>RAJBHAVAN ROAD,SOMAJIGUDA,HYD-500082</td>-->\n                        <!--</tr>-->\n                        <!--<tr>-->\n                             <!--<td>BRANCH CODE</td>-->\n                        <!--     <td>446</td>-->\n                        <!--</tr>-->\n                        <!--<tr>-->\n                             <!--<td>BANK A/C NAME</td>-->\n                        <!--     <td>CYBERABAD CONVENTION CENTER PVT.LTD</td>-->\n                        <!--</tr>-->\n                         <!--<tr>-->\n                             <!--<td>BANK A/C NO.</td>-->\n                         <!--    <td>5435456456456</td>-->\n                         <!--</tr>-->\n                        <!-- <tr>-->\n                        <!--     <td>IFSC CODE</td>-->\n        