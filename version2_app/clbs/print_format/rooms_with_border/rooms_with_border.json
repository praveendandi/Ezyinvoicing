 "html": "<!DOCTYPE html>\n<html lang=\"en\">\n<head>\n    <meta charset=\"UTF-8\">\n    <meta http-equiv=\"X-UA-Compatible\" content=\"IE=edge\">\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n     <title style=\"text-align:center\">Rooms</title>\n    \n    <style>\n    \n        body{\n            margin: 0;\n            padding: 0;\n        }\n        \n        .print-format {\n            font-size: 9pt;\n            font-family: Inter, \"Helvetica Neue\", Helvetica, Arial, \"Open Sans\", sans-serif;\n            -webkit-print-color-adjust: exact;\n            padding: 0;\n            margin: 0;\n        }\n        \n        p {\n            margin: 0;\n        }\n        \n        h3 {\n            margin: 0;\n            line-height: 20px;\n        }\n        \n        .d-flex {\n            display: flex;\n            flex-wrap: nowrap;\n        }\n\n        .d-flex > div{\n            margin-right: 20px;\n        }\n        \n        .mt-1{\n            margin-top: 10px;\n        }\n        \n        .text-right{\n            text-align:right;\n        }\n        .text-center{\n            text-align:center;\n        }\n        .row{\n            display : flex;\n        }\n        \n        .print-table{\n            border: 0;\n        }\n        \n       .print-table table{\n           width: 100%;\n           border-collapse: collapse;\n       }\n       \n       .print-table table, .print-table th, .print-table td{\n            border: 1px solid #666;\n        }\n        \n        .print-table th, .print-table td {\n          padding: 5px;\n          box-shadow: none;\n        }\n        \n        .printable2 table {\n            width: 100%;\n        }\n        \n        .printable2 th, .printable2 td{\n            border: none;\n        }\n        \n        .printable3 table{\n             width:100% !important;\n            /*min-width : 80%;*/\n            /*max-width : 95%;*/\n        }\n        .printable3 table, .printable3 th, .printable3 td{\n            border: solid 1px black !important;\n            border-collapse: collapse;\n            \n        }\n        .printable3 table thead tr th{\n            padding: 2px 4px !important;\n        }\n        .printable3 table tr td{\n            padding: 2px 4px !important;\n        }\n        \n        .print-friendly{\n            margin-top: 10px;\n        }\n        table.print-friendly tr td, table.print-friendly tr th {\n        page-break-inside: avoid;\n        }\n        \n        }\n        .totalamount{\n          border : solid 1px black;\n        }\n         @page{\n            size:landscape; \n            margin: 1cm; \n         }\n        #footer-html{\n            position:fixed;\n            bottom:-30px;\n        }\n    </style>\n</head>\n<body>\n    <div class=\"print-format\">\n        {% set doc_data =  frappe.db.sql(\"\"\"select DATE_FORMAT(`tabSummaries`.from_date,'%d-%m-%Y') as from_date,`tabSummaries`.terms_and_conditions,`tabSummaries`.po_number,`tabSummaries`.footer,`tabTaxPayerDetail`.legal_name,`tabTaxPayerDetail`.company,`tabTaxPayerDetail`.gst_number,`tabTaxpayer Locations`.location,`tabTaxpayer Locations`.address,`tabTaxpayer Locations`.pin_code,`tabTaxPayerDetail`.state_code,`tabTaxpayer Locations`.city from `tabSummaries` inner join `tabTaxPayerDetail` on `tabTaxPayerDetail`.gst_number=`tabSummaries`.tax_payer_details inner join `tabTaxpayer Locations` on `tabTaxpayer Locations`.name=`tabSummaries`.location where `tabSummaries`.tax_payer_details='{}' and `tabSummaries`.name='{}'\"\"\".format(tax_payer_details,name),as_dict=1)%}\n        {% set doc_data = doc_data|first %}\n            \n        {% set company_details=frappe.db.get_value('company',{'name':doc_data.company},['summary_header','summary_terms_and_conditions','company_name','summary_footer'],as_dict=1)%}\n        <div class='print-table'>\n            <table class='table'>\n                <tr>\n                    <td colspan='2'>\n                        <div class='d-flex'>\n                            <div>\n                                <img src={{base}} style=\"width: 150px; \"/>\n                            </div>\n                            <div>\n                                <h2 style='margin-b mt-1'>{{company_details.company_name}}</h2>\n                            </div>\n                        </div>\n                    </td>\n                </tr>\n                <tr>\n                    <td colspan='2' style='text-align: center'>\n                        <h3 style=\"text-align: center;\">Rooms</h3>\n                    </td>\n                </tr>\n                <tr>\n                    <td style=\"width: 50%\">\n                        <p class=\"mb-0\">Date :- {{ frappe.format_value(frappe.utils.now(), {'fieldtype': 'Date'}) }}</p>\n                    </td>\n                    <td style=\"width: 50%; text-align: right\">\n                        <p class='mb-0'>PO Number: {{doc_data.po_number}}</p>\n                    </td>\n                </tr>\n                <tr>\n                    <td colspan='2'>\n                        <p class=\"m-0\">To,</p>\n                        <p>\n                          <b class=\"fw-bold\">{{doc_data.legal_name}}</b><br>\n                           {{doc_data.legal_name}} <br>\n                            {%set items=[doc_data.address,doc_data.location,doc_data.city,doc_data.state,doc_data.pin_code]%}\n                            {{ items|reject('equalto', None)|join(',') }}\n                        </p>\n                        <p>\n                            <b>GST Number :</b> {{doc_data.gst_number}}<br>\n                            <b>State Code :</b> {{doc_data.state_code}}\n                        </p>\n                    </td>\n                </tr>\n                <tr>\n                  <td colspan='2'>\n                    <p>\n                        Dear Sir/ Madam, <br>\n                        Sub: Please find enclosed the following bills for settlement\n                    </p>\n                  </td>\n                </tr>\n            </table>\n        </div>\n        \n        <!--<div class='printable2'>-->\n        <!--    <table class='table'>-->\n        <!--        <tr>-->\n        <!--            <td colspan='2' style='text-align: center'>-->\n        <!--                <h3 style=\"text-align: center\">Rooms</h3>-->\n        <!--            </td>-->\n        <!--        </tr>-->\n        <!--        <tr>-->\n        <!--            <td style=\"width: 50%\">-->\n        <!--                <p class=\"mb-0\">Date :- {{ frappe.format_value(frappe.utils.now(), {'fieldtype': 'Date'}) }}</p>-->\n        <!--            </td>-->\n        <!--            <td style=\"width: 50%; text-align: right\">-->\n        <!--                <p class='mb-0'>PO Number: {{doc_data.po_number}}</p>-->\n        <!--            </td>-->\n        <!--        </tr>-->\n        <!--        <tr>-->\n        <!--            <td colspan='2'>-->\n        <!--                 <p class=\"m-0\">To,</p>-->\n        <!--                <p>-->\n        <!--                    <b class=\"fw-bold\">{{doc_data.legal_name}}</b><br>-->\n        <!--                   {{doc_data.legal_name}} <br>-->\n        <!--                    {%set items=[doc_data.address,doc_data.location,doc_data.city,doc_data.state,doc_data.pin_code]%}-->\n        <!--                    {{ items|reject('equalto', None)|join(',') }}-->\n        <!--                </p>-->\n        <!--            </td>-->\n        <!--        </tr>-->\n        <!--        <tr>-->\n        <!--            <td colspan='2'>-->\n        <!--            <p>-->\n        <!--                GST Number : {{doc_data.gst_number}}<br>-->\n        <!--                State Code : {{doc_data.state_code}}-->\n        <!--            </p>-->\n            \n        <!--            <p style=\" margin-top:15px\">-->\n        <!--                Dear Sir/ Madam, <br>-->\n        <!--                Sub: Please find enclosed the following bills for settlement-->\n        <!--            </p>-->\n        <!--            </td>-->\n        <!--        </tr>-->\n        <!--    </table>-->\n        <!--</div>-->\n        \n        <!--<h2 style=\"text-align:center;font-size:25px\">{{company_details.company_name}}</h2>-->\n        <!--<h4  style=\"text-align:center;font-size:18px\" >Rooms</h4>-->\n    \n       <div>\n            {% set doc_data = frappe.db.sql(\"\"\"select DATE_FORMAT(`tabSummaries`.from_date,'%d-%m-%Y') as from_date,`tabSummaries`.terms_and_conditions,`tabSummaries`.footer,`tabTaxPayerDetail`.legal_name,`tabTaxPayerDetail`.company,`tabTaxPayerDetail`.gst_number,`tabTaxpayer Locations`.location,`tabTaxpayer Locations`.address,`tabTaxpayer Locations`.pin_code,`tabTaxPayerDetail`.state_code,`tabTaxpayer Locations`.city from `tabSummaries` inner join `tabTaxPayerDetail` on `tabTaxPayerDetail`.gst_number=`tabSummaries`.tax_payer_details inner join `tabTaxpayer Locations` on `tabTaxpayer Locations`.name=`tabSummaries`.location where `tabSummaries`.tax_payer_details='{}' and `tabSummaries`.name='{}'\"\"\".format(tax_payer_details,name),as_dict=1)%} \n            <!--{% set doc_data=doc_data[0]%}-->\n            <!--frappe.db.get_all(doctype=\"Summaries\",fields=[`Summaries`.from_date,`TaxPayerDetail`.legal_name,`tabTaxPayerDetail`.gst_number\",\"`tabTaxPayerDetail`.address_1\",\"`tabTaxPayerDetail`.address_2\",\"`tabTaxPayerDetail`.pincode\",\"`tabTaxPayerDetail`.state_code\",\"`TaxPayerDetail`.location\"])%}-->\n            <!--<p class=\"fw-bold\">Date :- {{ frappe.format_value(frappe.utils.now(), {'fieldtype': 'Date'}) }}</p>-->\n            <!--<p class=\"fw-bold\">{{doc_data.guest_name}}</p>-->\n        \n       \n        <!--<p class=\"m-0\">To,</p>-->\n        <!--<p>-->\n        <!--    <b class=\"fw-bold\">{{doc_data.legal_name}}</b><br>-->\n        <!--   {{doc_data.legal_name}} <br>-->\n        <!--    